{
    "name": "pjatk-edux-checker",
    "version": "1.0.0",
    "description": "An easy way to see new notifications on the EDUX platform.",
    "main": "cli.js",
    "scripts": {
        "config": "./bin/cli.js --init",
        "start": "./bin/cli.js",
        "lint": "tsc --noEmit -p . && tslint \"*.ts\"",
        "test": "tsc --noEmit -p . && jest --no-cache"
    },
    "bin": {
        "check-edux": "./bin/cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lukaszklis/pjatk-edux-crawler.git"
    },
    "author": "Łukasz Kliś <lukasz@klis.pl>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/lukaszklis/pjatk-edux-crawler/issues"
    },
    "homepage": "https://github.com/lukaszklis/pjatk-edux-crawler#readme",
    "dependencies": {
        "@types/configstore": "^2.1.1",
        "@types/crypto-js": "^3.1.38",
        "@types/inquirer": "^0.0.36",
        "@types/jest": "^22.1.2",
        "chalk": "^2.3.1",
        "chromeless": "^1.5.0",
        "commander": "^2.14.1",
        "configstore": "^3.1.1",
        "crypto-js": "^3.1.9-1",
        "inquirer": "^5.1.0",
        "jest": "^22.3.0",
        "ts-jest": "^22.0.4",
        "ts-node": "^5.0.1",
        "tslint": "^5.9.1",
        "typescript": "^2.6.2"
    },
    "jest": {
        "transform": {
            ".ts": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "-spec\\.ts$",
        "moduleFileExtensions": [
            "ts",
            "js"
        ]
    }
}
